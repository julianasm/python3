for i in range(1, 11): #vai de 1 ate 10
    print('i = {}'.format(i))

for j in range(10):
    print(f'j = {j}')

for x in range(1, 11):
    for y in range(1, 11):
        print (f'{x} * {y} = {x * y}' )
         #interpola x e y
         #dentro das chaves é que realmente vai acontecer a multiplicação

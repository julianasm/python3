#numeros 02
from decimal import Decimal, getcontext  #decimal nao esta dentro do builtins entao é preciso importa-lo
print(Decimal(1) / Decimal(7))

getcontext(
).prec = 4  #get contex recebe 4, portanto a precisão sera de 4 casas decimais
print(Decimal(1) / Decimal(7))

print(Decimal.max(Decimal(1), Decimal(7)))
print(1.1 + 2.2)
getcontext().prec = 10
print(Decimal(1.1) + Decimal(2.2))
